[application]

config/name="Ninja Ball 2"
config/description="Ninja Ball 2 sequel to the first one"
run/main_scene="res://scenes/MainMenu.tscn"
config/use_custom_user_dir=true
config/icon="res://BackGrounds/bg_ninja_redBull.png"

[autoload]

